!bake
mkdir -p .built
g++ -fPIC -MMD -MF .built/test-test.d -c test.cpp -o .built/test-test.o
g++ -o libtest.so.0 -shared .built/test-test.o
ar -cq libtest.a .built/test-test.o
(exit SUCCESS)
!bake install
mkdir -p /usr/lib/test
cp libtest.so /usr/lib/test/libtest.so
cp libtest.so.0 /usr/lib/test/libtest.so.0
cp libtest.a /usr/lib/test/libtest.a
(exit SUCCESS)
!bake uninstall
rm -f /usr/lib/test/libtest.so
rm -f /usr/lib/test/libtest.so.0
rm -f /usr/lib/test/libtest.a
(exit SUCCESS)
!bake clean
rm -f libtest.so.0
rm -f libtest.a
rm -rf .built/
rm -f libtest.so
(exit SUCCESS)

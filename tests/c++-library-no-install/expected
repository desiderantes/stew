!bake
mkdir -p .built
g++ -fPIC -MMD -MF .built/test-test.d -c test.cpp -o .built/test-test.o
g++ -o libtest.so.0 -shared .built/test-test.o
ar -cq libtest.a .built/test-test.o
(exit SUCCESS)
!bake install
(exit SUCCESS)
!bake uninstall
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f libtest.so.0
rm -f libtest.a
rm -f libtest.so
(exit SUCCESS)

!bake
mkdir -p .built
gcc -fPIC -MMD -MF .built/test-test.d -c test.c -o .built/test-test.o
gcc -o libtest.so -shared .built/test-test.o
ar -cq libtest.a .built/test-test.o
(exit SUCCESS)
!bake install
(exit SUCCESS)
!bake uninstall
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f libtest.so
rm -f libtest.a
(exit SUCCESS)

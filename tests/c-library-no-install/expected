!bake
mkdir -p .built
gcc -fPIC -MMD -MF .built/test.d -c test.c -o .built/test.o
gcc -o libtest.so.0 -shared .built/test.o
ar -cq libtest.a .built/test.o
(exit SUCCESS)
!bake install
(exit SUCCESS)
!bake uninstall
(exit SUCCESS)
!bake clean
rm -f libtest.so.0
rm -f libtest.a
rm -rf .built/
rm -f libtest.so
rm -f test-0.pc
(exit SUCCESS)

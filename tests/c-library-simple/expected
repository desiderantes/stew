!bake
mkdir -p .built
gcc -fPIC -MMD -MF .built/test.d -c test.c -o .built/test.o
gcc -o libtest.so.0 -shared .built/test.o
ar -cq libtest.a .built/test.o
(exit SUCCESS)
!bake install
mkdir -p /usr/lib
cp libtest.so /usr/lib/libtest.so
cp libtest.so.0 /usr/lib/libtest.so.0
cp libtest.a /usr/lib/libtest.a
(exit SUCCESS)
!bake uninstall
rm -f /usr/lib/libtest.so
rm -f /usr/lib/libtest.so.0
rm -f /usr/lib/libtest.a
(exit SUCCESS)
!bake clean
rm -f libtest.so.0
rm -f libtest.a
rm -rf .built/
rm -f libtest.so
(exit SUCCESS)

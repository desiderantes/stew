!bake
mkdir -p .built
gcc -Ibaketestlibrary -MMD -MF .built/test-test.d -c test.c -o .built/test-test.o
gcc -o test .built/test-test.o -lbaketestlibrary
(exit SUCCESS)
!bake install
mkdir -p /usr/bin
cp test /usr/bin/test
(exit SUCCESS)
!bake uninstall
rm -f /usr/bin/test
(exit SUCCESS)
!bake clean
rm -f test
rm -rf .built/
(exit SUCCESS)

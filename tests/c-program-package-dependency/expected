!bake
mkdir -p .built
gcc -Ibaketest-dependency -Ibaketest-library -MMD -MF .built/test-test.d -c test.c -o .built/test-test.o
gcc -o test .built/test-test.o -lbaketest-dependency -lbaketest-library
(exit SUCCESS)
!bake install
mkdir -p /usr/bin
cp test /usr/bin/test
(exit SUCCESS)
!bake uninstall
rm -f /usr/bin/test
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f test
(exit SUCCESS)

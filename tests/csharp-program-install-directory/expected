!bake
gmcs -out:Test.exe Test.cs
mkdir -p .built
(exit SUCCESS)
!bake install
mkdir -p /usr/share/test-project
cp Test.exe /usr/share/test-project/Test.exe
mkdir -p /usr/bin/test
cp .built/Test /usr/bin/test/Test
(exit SUCCESS)
!bake uninstall
rm -f /usr/share/test-project/Test.exe
rm -f /usr/bin/test/Test
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f Test.exe
rm -f Test
(exit SUCCESS)

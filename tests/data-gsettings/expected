!bake
mkdir .built
(exit 0)
!bake install
mkdir -p /usr/local/share/glib-2.0/schemas
install com.example.Test.gschema.xml /usr/local/share/glib-2.0/schemas/com.example.Test.gschema.xml
install test.override /usr/local/share/glib-2.0/schemas/test.override
mkdir -p /usr/local/share/GConf/gsettings
install test.convert /usr/local/share/GConf/gsettings/test.convert
(exit 0)

!bake
mkdir -p .built
bake-gettext --mime-type application/x-glade --output .built/test.ui.pot test.ui
msgcat --force-po --output-file=test.pot .built/test.ui.pot
(exit SUCCESS)
!bake install
mkdir -p /usr/local/share/test/ui
cp test.ui /usr/local/share/test/ui/test.ui
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f test.pot
(exit SUCCESS)

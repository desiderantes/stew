!bake
(exit SUCCESS)
!bake install
mkdir -p /usr/share/help/C/test
cp index.page /usr/share/help/C/test/index.page
cp test.page /usr/share/help/C/test/test.page
(exit SUCCESS)
!bake uninstall
rm -f /usr/share/help/C/test/index.page
rm -f /usr/share/help/C/test/test.page
(exit SUCCESS)
!bake clean
(exit SUCCESS)

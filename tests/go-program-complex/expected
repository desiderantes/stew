!bake
mkdir -p .built
gccgo -c test1.go -o .built/test1.o
gccgo -c test2.go -o .built/test2.o
gccgo -c test3.go -o .built/test3.o
gccgo -o test .built/test1.o .built/test2.o .built/test3.o
(exit SUCCESS)
!bake install
mkdir -p /usr/local/bin
cp test /usr/local/bin/test
(exit SUCCESS)
!bake clean
rm -f test
rm -rf .built/
(exit SUCCESS)

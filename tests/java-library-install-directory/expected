!bake
mkdir -p .built
javac -d .built Test.java
jar cfe test.jar -C .built Test.class
(exit SUCCESS)
!bake install
mkdir -p /usr/share/java
cp test.jar /usr/share/java/test.jar
(exit SUCCESS)
!bake uninstall
rm -f /usr/share/java/test.jar
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f test.jar
(exit SUCCESS)

!bake
python --version
python -m py_compile test1.py
python -m py_compile test2.py
python -m py_compile test3.py
mkdir -p .built
bake-gettext --language Python --output .built/test1.py.pot test1.py
bake-gettext --language Python --output .built/test2.py.pot test2.py
bake-gettext --language Python --output .built/test3.py.pot test3.py
msgcat --force-po --output-file=test.pot .built/test1.py.pot .built/test2.py.pot .built/test3.py.pot
(exit SUCCESS)
!bake install
python --version
mkdir -p /usr/local/lib/python2.7/site-packages/test
cp test1.pyc /usr/local/lib/python2.7/site-packages/test/test1.pyc
cp test2.pyc /usr/local/lib/python2.7/site-packages/test/test2.pyc
cp test3.pyc /usr/local/lib/python2.7/site-packages/test/test3.pyc
(exit SUCCESS)
!bake clean
python --version
rm -f test1.pyc
rm -f test2.pyc
rm -f test3.pyc
rm -rf .built/
rm -f test.pot
(exit SUCCESS)

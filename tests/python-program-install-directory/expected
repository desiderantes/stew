!bake
python -m py_compile test.py
mkdir -p .built
(exit SUCCESS)
!bake install
mkdir -p /usr/share/test
cp test.pyc /usr/share/test/test.pyc
mkdir -p /usr/bin/test
cp .built/test /usr/bin/test/test
(exit SUCCESS)
!bake uninstall
rm -f /usr/share/test/test.pyc
rm -f /usr/bin/test/test
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f test.pyc
rm -f test
(exit SUCCESS)

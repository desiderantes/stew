!bake
mkdir -p __pycache__
python3.2 -m py_compile test.py
mkdir -p .built
(exit SUCCESS)
!bake install
mkdir -p /usr/share/test
cp test.py /usr/share/test/test.py
mkdir -p /usr/share/test/__pycache__
cp __pycache__/test.cpython-32.pyc /usr/share/test/__pycache__/test.cpython-32.pyc
mkdir -p /usr/bin
cp .built/test /usr/bin/test
(exit SUCCESS)
!bake uninstall
rm -f /usr/share/test/test.py
rm -f /usr/share/test/__pycache__/test.cpython-32.pyc
rm -f /usr/bin/test
(exit SUCCESS)
!bake clean
rm -f __pycache__/test.cpython-32.pyc
rm -rf __pycache__/
rm -f test
rm -rf .built/
(exit SUCCESS)

!bake
moc -o test.moc test.cpp
mkdir -p .built
g++ -MMD -MF .built/test-test.d -c test.cpp -o .built/test-test.o
g++ -o test .built/test-test.o
(exit SUCCESS)
!bake install
mkdir -p /usr/bin
cp test /usr/bin/test
(exit SUCCESS)
!bake uninstall
rm -f /usr/bin/test
(exit SUCCESS)
!bake clean
rm -rf .built/
rm -f test
rm -f test.moc
(exit SUCCESS)

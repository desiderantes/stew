!stew
mkdir -p .built
gcc -MMD -MF .built/test-test.d -c test.c -o .built/test-test.o
gcc -o test .built/test-test.o
(exit SUCCESS)
!stew install
mkdir -p /usr/bin
cp test /usr/bin/test
(exit SUCCESS)
!stew uninstall
rm -f /usr/bin/test
(exit SUCCESS)
!stew clean
rm -rf .built/
rm -f test
(exit SUCCESS)

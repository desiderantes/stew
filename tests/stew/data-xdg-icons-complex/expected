!stew
(exit SUCCESS)
!stew install
mkdir -p /usr/share/icons/HighContrast/32x32/actions
cp 32x32/test.png /usr/share/icons/HighContrast/32x32/actions/test.png
mkdir -p /usr/share/icons/HighContrast/64x64/actions
cp 64x64/test.png /usr/share/icons/HighContrast/64x64/actions/test.png
(exit SUCCESS)
!stew uninstall
rm -f /usr/share/icons/HighContrast/32x32/actions/test.png
rm -f /usr/share/icons/HighContrast/64x64/actions/test.png
(exit SUCCESS)
!stew clean
(exit SUCCESS)

!stew
mkdir -p .built
javac -d .built Test.java
jar cf test.jar -C .built Test.class
(exit SUCCESS)
!stew install
mkdir -p /usr/share/test-project
cp test.jar /usr/share/test-project/test.jar
mkdir -p /usr/bin/test
cp .built/test /usr/bin/test/test
(exit SUCCESS)
!stew uninstall
rm -f /usr/share/test-project/test.jar
rm -f /usr/bin/test/test
(exit SUCCESS)
!stew clean
rm -rf .built/
rm -f test.jar
rm -f test
(exit SUCCESS)

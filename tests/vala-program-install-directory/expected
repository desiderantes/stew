!bake
mkdir -p .built
valac --ccode test.vala
mv test.c .built/test-test.c
touch .built/test-test.c-stamp
gcc -Wno-unused -Wno-deprecated-declarations -Igobject -Iglib -c .built/test-test.c -o .built/test-test.o
gcc -o test .built/test-test.o -lgobject -lglib
(exit SUCCESS)
!bake install
mkdir -p /usr/bin/test
cp test /usr/bin/test/test
(exit SUCCESS)
!bake uninstall
rm -f /usr/bin/test/test
(exit SUCCESS)
!bake clean
rm -f test
rm -rf .built/
(exit SUCCESS)
